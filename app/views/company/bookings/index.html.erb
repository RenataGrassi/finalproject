<h1>teste</h1>

<div class="row">
  <div class="col-6">
  <%= simple_form_for :search, url: company_bookings_path, method: :get do |f| %>
    <%= f.input :start_date, as: :date, required: true %>
    <%= f.input :end_date, as: :date, required: true %>
    <%= f.submit "Search", class: "btn btn-primary" %>
  <% end %>
  </div>
</div>

<%= current_user.company.name %>
<%# current_user.company.products.first.activity %>
  <% @bookings.each do |booking| %>
   <li> <%=  booking.product.name %>
        R$ <%= booking.price %>
        <%= booking.canceled %>
        <% booking.guests.each do |guest| %>
          <%= guest.name %>
        <% end %>
        <%= booking.date %>
   </li>
<% end %>
<% console %>
