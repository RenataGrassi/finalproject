<div class="container">
<h2> Recent bookings </h2>
<div class="notifications-container">
  <% @notifications.each do |notification| %>
    <div class="notification">
      <div class="notification-content">
        <p><small><%= notification.created_at %></small></p>
        <p>
          <%= "#{notification.booking.shopping_cart.user.first_name} booked #{notification.booking.product.name}" %>
        </p>
      </div>
      <div class="notification-actions">
        <%= link_to "Go to bookings  ", company_bookings_path %><i class="fas fa-ship ml-1"></i>

      </div>
    </div>
      <% notification.read = true %>
      <% notification.save %>
  <% end %>
</div>
    <div class="m-5 digg_pagination">
    <%= will_paginate @notifications %>
  </div>
</div>
